{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "types": ["svelte", "vite/client"],
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "strict": true,
        "skipLibCheck": true,
        "noEmit": true
    },
  "include": ["src/**/*", "tests/**/*.ts", "tests/**/*.d.ts"]
}
